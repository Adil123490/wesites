<?xml version="1.0" encoding="utf-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="com.wechat.entity">
	<class name="Reply" table="wx_reply">
		<id name="id" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="keyword" column="keyword_" length="100"/>
		<property name="content" column="content" length="2000"/>
		<property name="createDate" column="createDate" type="timestamp" update="false"/>
	</class>
</hibernate-mapping>